{
  "name": "chatterbox",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "lint": "run-s lint:all:*",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write",
    "lint:svgo": "svgo",
    "lint:all:eslint": "npm run lint:eslint -- . --ignore-path .gitignore",
    "lint:all:prettier": "npm run lint:prettier -- . --ignore-path .gitignore",
    "lint:all:svgo": "svgo -f ./assets/icons",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.5.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "svgo": "^3.0.2",
    "tailwindcss": "^3.3.2"
  }
}
